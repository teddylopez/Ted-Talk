<h1 id="page_title">Projects</h1>

<div id="post_wrapper">
  <% @projects.each do |project| %>
    <div class="post">
       <p class="date"><%= project.created_at.strftime("%A, %B %d") %></p>
       <h2><%= link_to project.title, project %></h2>
       <hr>
    </div>
  <% end %>

<div class="paginate-wrap">
  <%= will_paginate @projects %>
</div>
