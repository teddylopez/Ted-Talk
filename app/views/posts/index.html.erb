<h1 id="page_title">Posts</h1>

<div id="post_wrapper">
  <% @posts.each do |post| %>
    <div class="post">
       <p class="date"><%= post.created_at.strftime("%A, %B %d") %></p>
       <h2><%= link_to post.title, post %></h2>
       <hr>
    </div>
  <% end %>

<div class="paginate-wrap">
  <%= will_paginate @posts %>
</div>
